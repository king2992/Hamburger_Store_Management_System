<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
<settings>
<setting name="mapUnderscoreToCamelCase" value="true"/>
</settings>

<typeAliases>
	<typeAlias type="kr.ac.kopo.model.User" alias="User"/>
	<typeAlias type="kr.ac.kopo.util.Pager" alias="Pager"/>
	<typeAlias type="kr.ac.kopo.model.Ticketing" alias="Ticketing"/>
	<typeAlias type="kr.ac.kopo.model.Seat" alias="Seat"/>
	<typeAlias type="kr.ac.kopo.model.reservedTime" alias="reservedTime"/>
	<typeAlias type="kr.ac.kopo.model.Review" alias="Review"/>
</typeAliases>

<mappers>
	<mapper resource="mybatis/User.xml"/>
	<mapper resource="mybatis/Ticketing.xml"/> 
	<mapper resource="mybatis/Seat.xml"/>
	<mapper resource="mybatis/review.xml"/>
</mappers>
</configuration>