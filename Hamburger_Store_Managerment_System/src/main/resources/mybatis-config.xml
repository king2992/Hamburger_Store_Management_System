<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
	<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>

	<typeAliases>
		<typeAlias type="kr.ac.kopo.model.User" alias="User"/>
		<typeAlias type="kr.ac.kopo.model.reservedTime" alias="reservedTime"/>
		<typeAlias type="kr.ac.kopo.model.Review" alias="Review"/>
		<typeAlias type="kr.ac.kopo.model.Comment" alias="Comment"/>
		<typeAlias type="kr.ac.kopo.model.TakeOutReserved" alias="TakeOutReserved" />
		<typeAlias type="kr.ac.kopo.model.Menu" alias="Menu" />
		<typeAlias type="kr.ac.kopo.model.Orders" alias="Orders" />
		<typeAlias type="kr.ac.kopo.model.ordersMenuList" alias="ordersMenuList" />
		<typeAlias type="kr.ac.kopo.model.TakeoutReservedMenu" alias="TakeoutReservedMenu" />
		<typeAlias type="kr.ac.kopo.model.setSideDrink" alias="setSideDrink" />
		<typeAlias type="kr.ac.kopo.model.PayInfo" alias="PayInfo" />
		<typeAlias type="kr.ac.kopo.model.Like" alias="Like" />
		<typeAlias type="kr.ac.kopo.model.Admin" alias="Admin" />
		<typeAlias type="kr.ac.kopo.model.BusinessNumCheck" alias="BusinessNumCheck" />
		<typeAlias type="kr.ac.kopo.model.MenuInfo" alias="MenuInfo" />
	</typeAliases>

	<mappers>
		<mapper resource="mybatis/Admin.xml"/>
		<mapper resource="mybatis/User.xml"/>
		<mapper resource="mybatis/Review.xml"/>
		<mapper resource="mybatis/Comment.xml" />
		<mapper resource="mybatis/File.xml" />
		<mapper resource="mybatis/Pos.xml" />
		<mapper resource="mybatis/TakeOutReservedList.xml" />
		<mapper resource="mybatis/MenuIntroduce.xml" />
	</mappers>
</configuration>